{
  "name": "@noxx/react-image-loader",
  "version": "2.0.0",
  "description": "A React component that preloads images via img or picture tags",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "deploy": "gh-pages -d ./example/dist",
    "lint": "eslint ./src/**/*.js",
    "prepublishOnly": "npm run transpile && npm run wp:build && npm run deploy",
    "start": "cross-env BABEL_ENV=cjs PORT=9000 concurrently --kill-others -p {name} -n WEBPACK,SERVER -c black.bgCyan,black.bgMagenta \"npm run wp:watch\" \"npm run start:server\"",
    "start:server": "babel-node ./example/watcher",
    "test": "yarn lint && yarn unit",
    "transpile": "npm run transpile:cjs && npm run transpile:esm",
    "transpile:cjs": "cross-env BABEL_ENV=cjs babel ./src -d cjs --copy-files",
    "transpile:esm": "cross-env BABEL_ENV=esm babel ./src -d esm --copy-files",
    "unit": "cross-env BABEL_ENV=cjs jest --env=jsdom --config ./.jest/conf.jest.js",
    "unit:watch": "npm run unit -- --watchAll",
    "wp:build": "BABEL_ENV=cjs webpack --mode production",
    "wp:watch": "BABEL_ENV=cjs webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the0neWhoKnocks/react.image-loader.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the0neWhoKnocks/react.image-loader/issues"
  },
  "homepage": "https://github.com/the0neWhoKnocks/react.image-loader#readme",
  "peerDependencies": {
    "emotion": "^10.0.7",
    "prop-types": "^15.6.1",
    "react": "^16.3.3",
    "react-dom": "^16.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@noxx/eslint-config": "^1.0.7",
    "@noxx/webpack-tidy-plugin": "^2.0.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "emotion": "^10.0.7",
    "emotion-server": "^10.0.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint-plugin-require-jsdoc-except": "^1.3.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.3",
    "react-dom": "^16.3.3",
    "simple-progress-webpack-plugin": "^1.1.2",
    "webpack": "^4.24.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.1.2"
  }
}
